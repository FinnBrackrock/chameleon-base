<div id="successQueryContainer-{{ bundleName }}-{{ buildNumber }}" class="successQueryContainer">
    <div class="head">
        <span class="message">{{ 'chameleon_system_core.cms_module_update.show_successful_queries'|trans }}</span> (<span class="count">{{ messages|length }}</span>)
    </div>
    <ul class="queryList">
        {% for message in messages %}
            <li>
                {{ 'chameleon_system_core.cms_module_update.query'|trans }} <pre>{{ message.query }}</pre><br />
                {{ 'chameleon_system_core.cms_module_update.line'|trans }} {{ message.line }}
            </li>
        {% endfor %}
    </ul>
</div>
